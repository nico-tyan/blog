<?xml version="1.0" encoding="utf-8" ?>
<html>	                  
	 <!--99健康网       http://zyk.99.com.cn/baike/683858.htm  详细页-->
	<forurlReg><![CDATA[http://[\S]+.99.com.cn[\S]*]]></forurlReg>
	<load_on_items>
	    <item name="siteName" mode="str">
   			<![CDATA[99健康网]]>
		</item>
		<item name="type" mode="str">
   			<![CDATA[2]]> <!-- 1 搜索列表  2 详情 -->
		</item>
		<item name="charset" mode="str">
   			<![CDATA[gbk]]>
		</item>
		<item name="url_charset" mode="str">
   			<![CDATA[utf-8]]>
		</item>
		<item name="statusCode" mode="str">
   			<![CDATA[200]]><!--下载返回码为 200 视为成功 -->
		</item>
		<item name="cycle_retry_times" mode="str">
   			<![CDATA[1]]><!-- 重试次数 -->
		</item>	
		<!--内容解析成功必须存在的几个 值 -->
		<item name="exist_keys" mode="str">
   			<![CDATA[class]]>
		</item>
		<item name="first_keys" mode="str">
   			<![CDATA[class]]>
		</item>
	</load_on_items>
	<!-- 临时集合  页面下载后获取-->
	<tmp_items>
		<item name="class" mode="regular" type="tmp">
   			<![CDATA[class]]>
		</item>
	</tmp_items>
	<!--内容块 -->
	<Contents>
		<!--标题 -->	
		<item name="Title" mode="xpath">
    		<![CDATA[//div[@class="art_nav"]/h1/text()]]>
		</item>
		
		<!--作者-->		
		<item name="pubAuthor" mode="xpath" type="tmp">
    	<![CDATA[//div[@class="art_cont"]/html()]]>
		</item> 
		<item name="Author" mode="regular" parent="pubAuthor">
    	     <![CDATA[责任编辑：(.*?)\)]]>   	     
		</item>		
						
		<!--发布时间 datetimeformat时间格式 多个以 ， , | 或空格隔开 -->
		<item name="DateTime" mode="gen" rule="6" >
			<![CDATA[{empty}]]>
		</item> 
		
		<item name="pubDateTime" mode="regular" parent="DateTime" dateformat="yyyy-MM-dd">
    	     <![CDATA[(\d+-\d+-\d+)]]>
		</item>		
		
		<!--来源 -->	
		<item name="pubFrom" mode="xpath" type="tmp">
 			<![CDATA[]]>
		</item>	
		<item name="From" mode="regular" parent="pubFrom">
    	    <![CDATA[]]>
		</item>		
		
		<item name="ClickCount11" mode="xpath" type="tmp">
   	    	 <![CDATA[]]>   	     
		</item>
		<item name="ClickCount" mode="regular" parent="ClickCount11">
    	       <![CDATA[(\d+)]]>   	     
		</item>	
		
		<item name="ReplyCount11" mode="xpath" type="tmp">
    	     <![CDATA[]]>   	     
		</item>
		<item name="ReplyCount" mode="regular" parent="ReplyCount11">
    	        <![CDATA[(\d+)]]>   	 
		</item>	
		
		<!--摘要 -->	
		<item name="Summary" mode="xpath"><!-- 摘要 -->
			<![CDATA[]]>
		</item>		
		
		<!--内容-->	
		<item name="Content" mode="xpath"><!--内容 -->
    		<![CDATA[//div[@class="art_cont"]/html()]]>
		</item>		
	</Contents>		
	<page>
		<!--翻页 下一页规则 -->
		<!-- 第一种方式 --> 
		<item name="page" mode="regular" type="tmp">
   			<![CDATA[]]> 
		</item>
		<!-- 第二种方式 -->
		<item name="currentPage" mode="regular" type="tmp">
   			<![CDATA[]]>
		</item>
		<item name="countPage" mode="regular" type="tmp">
   			<![CDATA[]]>
		</item>
		<!-- append_page = 0 or 1   时 递增规则 0 或 1 开始 --> 
		<url_join ref="next_url" append_page="1"><![CDATA[_]]></url_join>
		<url_join ><![CDATA[]]></url_join>
	</page>
	<!-- 翻页 下一页内容 -->
	<Next_Contents>
		<item name="Content" mode="xpath"><!-- 内容 -->
    		<![CDATA[]]>
		</item>
	</Next_Contents>
</html>