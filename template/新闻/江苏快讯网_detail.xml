<?xml version="1.0" encoding="utf-8" ?>
<html>
	<!-- 江苏快讯网http://news.jsdushi.net/2017/1125/175726.shtml -->
	<forurlReg><![CDATA[http://news.jsdushi.net/[\S]+]]></forurlReg>
	<!-- 临时集合 页面下载前获取-->
	<load_on_items>
		<item name="siteName" mode="str">
   	 		<![CDATA[江苏快讯网]]>
		</item>
		<item name="type" mode="str">
   			<![CDATA[2]]> <!-- 1 搜索列表  2 详情 -->
		</item>
		<item name="charset" mode="str">
   			<![CDATA[utf-8]]>
		</item>
		<item name="url_charset" mode="str">
   			<![CDATA[utf-8]]>
		</item>
		<item name="statusCode" mode="str">
   			<![CDATA[200]]><!--下载返回码为 200 视为成功 -->
		</item>
		<item name="cycle_retry_times" mode="str">
   			<![CDATA[1]]><!-- 重试次数 -->
		</item>
		<!--内容解析成功必须存在的几个 值 -->
		<item name="exist_keys" mode="str">
   			<![CDATA[baseLayout]]>
		</item>
	</load_on_items>
	<!-- 临时集合  页面下载后获取-->
	<tmp_items>
		<!-- 能够获取到列表块 视为此模板匹配成功 -->
		<item name="baseLayout" mode="xpath" type="tmp">
   			<![CDATA[//div[@class='baseLayout']/@class]]>
		</item> 
	</tmp_items>
	<Contents>
		<!--标题 -->
		<item name="Title" mode="xpath">
    	<![CDATA[//div[@class='neiHeader']/h1/text()]]>
		</item>
		<item name="tmp_From" mode="xpath" type="tmp" >
    	     <![CDATA[//p[@class='leftq']/span[2]/text()]]>   	     
		</item>
		<item name="From" mode="regular"  parent="tmp_From">
    	     <![CDATA[来源：(.*?)$]]>   	     
		</item>
		<!--发布时间 datetimeformat时间格式 多个以 ， , | 或空格隔开 -->
		<item name="tmp_pubDateTime" mode="xpath" type="tmp">
    	<![CDATA[//p[@class='leftq']/span[@id='pubtime_baidu']/text()]]>
		</item>
		<item name="pubDateTime" mode="regular" dateformat="yyyy-MM-dd HH:mm:ss"
			parent="tmp_pubDateTime">
    	<![CDATA[^(.*?)$]]>
		</item>
		<item name="Content" mode="xpath" ><!--内容 -->
    	<![CDATA[//div[@id='textContent']/html()]]>
		</item>
	</Contents>
</html>